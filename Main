#include<stdio.h>
#include<cstring>
int main()
{
	char a[30][3]={
		{'.','_','.'},
		{'.','.','.'},
		{'.','_','.'},
		{'.','_','.'},
		{'.','.','.'},
		{'.','_','.'},
		{'.','_','.'},
		{'.','_','.'},
		{'.','_','.'},
		{'.','_','.'},
		{'|','.','|'},
		{'.','.','|'},
		{'.','_','|'},
		{'.','_','|'},
		{'|','_','|'},
		{'|','_','.'},
		{'|','_','.'},
		{'.','.','|'},
		{'|','_','|'},
		{'|','_','|'},
		{'|','_','|'},
		{'.','.','|'},
		{'|','_','.'},
		{'.','_','|'},
		{'.','.','|'},
		{'.','_','|'},
		{'|','_','|'},
		{'.','.','|'},
		{'|','_','|'},
		{'.','.','|'}
	};
	int num=0,i=1,j=1,c=0,e=0,f,d,g,h;
	int sum=0;
	int b[50];
	printf("input:");
	scanf("%d",&num);
	sum=num;
	while(j)
	{
		sum=sum/10;
		if(sum==0)
		{
			j=0;
			break;
		}
		i++;
	}


	for(c=0;c<i;c++)
	{
		b[c]=num%10;
