	}
	for(e=0;e<3;e++)
{
	for(f=0;f<i;f++)
	{
		for(d=0;d<3;d++)
		{
			g=(i-f-1);
			printf("%c",a[b[g]+(e*10)][d]);
		}
		printf(" ");
	
	}
	printf("\n");
}

	
	return 0;
